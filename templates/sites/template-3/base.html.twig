          href="{{ asset('templates/template-3/assets/css/revolution_settings.css') }}">
    <link rel="stylesheet" href="{{ asset('templates/template-3/assets/css/bootstrap.css') }}">
    <link rel="stylesheet"
          href="{{ asset('templates/template-3/assets/css/font-awesome.css') }}">
    <link rel="stylesheet" href="{{ asset('templates/template-3/assets/css/axfont.css') }}">
