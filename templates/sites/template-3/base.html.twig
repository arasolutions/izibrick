          type='text/css'>
    <style type="text/css">
        :root {
            --colorTheme: {{ site.colorTheme }};
            --textColor: {{ site.textColor }};
            --headerTextColor: {{ site.lightTheme ? '#333' : 'white'  }};
            --headerBackgroundColor: {{ site.lightTheme ? 'white' : '#333'  }};
            --footerTextColor: {{ site.lightTheme ? '#333' : 'white'  }};
            --footerBackgroundColor: {{ site.lightTheme ? 'white' : '#333'  }};
        }
    </style>
    <!--[if lte IE 11]>
    <style type="text/css">
        ::selection {
            background: {{ site.colorTheme }} !important;
        }

        .navigation > li:hover > a,
        .navigation > li > a:hover,
        .navigation > li > .activelink {
            color: {{ site.colorTheme }} !important;
        }

        .navigation > li > a{
            color: {{ site.lightTheme ? '#333' : 'white'  }} !important;
        }

        .body-wrapper a:hover,
        .top-body a:hover,
        .top-footer a:hover {
            color: {{ site.colorTheme }} !important;
        }

        .blog-post-details-item.blog-post-details-item-right a:hover i {
            color: {{ site.colorTheme }} !important;
        }

        #header {
            background-color: {{ site.lightTheme ? 'white' : '#333'  }};
            color: {{ site.lightTheme ? '#333' : 'white'  }};
        }

        #footer {
            background: {{ site.lightTheme ? 'white' : '#333'  }};
            color: {{ site.lightTheme ? '#333' : 'white'  }} !important;
        }

        .button, .body-wrapper input[type="submit"], .body-wrapper input[type="button"], .section-content.section-color-bg, .content-box.content-style4 h4 i, button.button-main, .body-wrapper .tags a:hover, .callout-box.callout-box2, .blog-search .blog-search-button, .top-title-wrapper, .testimonial-big,
        .content-style3:hover .content-style3-icon, table.table thead tr, .price-table .price-label-badge, .price-table .price-table-header, .section-subscribe .subscribe-button.icon-envelope-alt {
