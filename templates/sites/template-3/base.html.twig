            color: {{ site.textColor }} !important;
        }

        .blog-post .blog-post-type {
            background-color: {{ site.colorTheme }} !important;
            color: {{ site.textColor }} !important;
        }


        .h1-page-title, .h2-page-desc {
            color: {{ site.textColor }} !important;
        }

        .btn, input[type="submit"], input[type="button"], button.btn, .btn-izibrick {
            background-color: {{ site.colorTheme }} !important;
            color: {{ site.textColor }} !important;
        }

        .copyright{
            background-color: {{ site.colorTheme }} !important;
        }
    </style>
    <![endif]-->
    <link href="{{ asset('templates/template-3/assets/css/izibrick.css') }}"
          rel='stylesheet' type='text/css' id="skin-file">
    <script src="https://maps.googleapis.com/maps/api/js?v=3.exp&amp;sensor=false"></script>

    <!-- Fonts -->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,600' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Raleway:400,100,200,300,500,600' rel='stylesheet'
          type='text/css'>

    <!--[if lt IE 9]>
    <script type="text/javascript"
            src="{{ asset('templates/template-3/assets/js/respond.js') }}"></script>
    <![endif]-->
</head>
<body>
<div id="wrapper">

    <div class="top_wrapper">

        <!-- Header -->
        <header id="header">
            <div class="container">

                {% set siteName = site.internalName|param(app.request.server.get('REQUEST_URI')) %}

                <div class="row header">

                    <!-- Logo -->
                    <div class="col-xs-4 logo">
                        <a href="{{ path('home'|route(app.request.server.get('REQUEST_URI')), siteName) }}">
                            {% if site.logo %}
                                <img alt="Logo {{ site.name }}" height="35"
                                     src="{{ vich_uploader_asset(site, 'logoFile') | imagine_filter('logo_template') }}"/>
                            {% else %}
                                <img alt="Logo {{ site.name }}" height="35"
                                     src="{{ asset('assets/img/logo_site_default.png') }}"/>
                            {% endif %}
                        </a>
                        {% if not site.nameInLogo %}
                            <span class="logo-title">{{ site.name }}</span>
                        {% endif %}
                    </div>
                    <!-- //Logo// -->


                    <!-- Navigation File -->
                    <div class="col-md-8">

                        <!-- Mobile Button Menu -->
                        <div class="mobile-menu-button">
                            <i class="fa fa-list-ul"></i>
                        </div>
                        <!-- //Mobile Button Menu// -->

                        <nav>
                            <ul class="navigation">
                                <li>
                                    <a class="{% block indexActive %}{% endblock %}"
                                       href="{{ path('home'|route(app.request.server.get('REQUEST_URI')), siteName) }}">
                                        <span class="label-nav">
                                        Accueil
                                                </span>
                                    </a>
                                </li>
                                <li>
                                    <a class="{% block presentationActive %}{% endblock %}"
                                       href="{{ path('presentation'|route(app.request.server.get('REQUEST_URI')), siteName) }}">
                                        <span class="label-nav">
                                        Présentation
                                                </span>
                                    </a>
                                </li>
                                <li>
                                    <a class="{% block blogActive %}{% endblock %}"
                                       href="{{ path('blog'|route(app.request.server.get('REQUEST_URI')), siteName) }}">
                                        <span class="label-nav">
                                        Blog
                                                </span>
                                    </a>
                                </li>
                                {% if site.pricing.display == true %}
                                    <li>
                                        <a class="{% block tarifActive %}{% endblock %}"
                                           href="{{ path('tarif'|route(app.request.server.get('REQUEST_URI')), siteName) }}">
                                        <span class="label-nav">
                                        Tarif
                                                </span>
                                        </a>
                                    </li>
                                {% endif %}
                                {% if site.quote.display == true %}
                                    <li>
                                        <a class="{% block devisActive %}{% endblock %}"
                                           href="{{ path('devis'|route(app.request.server.get('REQUEST_URI')), siteName) }}">
                                            <span class="label-nav">
                                            Devis
                                                    </span>
                                        </a>
                                    </li>
                                {% endif %}
                                <li>
                                    <a class="{% block contactActive %}{% endblock %}"
                                       href="{{ path('contact'|route(app.request.server.get('REQUEST_URI')), siteName) }}">
                                        <span class="label-nav">
                                        Contact
                                                </span>
                                    </a>
                                </li>
                            </ul>

                        </nav>
                    </div>
                    <!-- Nav -->

                </div>


            </div>
        </header>
    </div><!--.top wrapper end -->

    <div class="loading-container">
        <div class="spinner">
            <div class="double-bounce1"></div>
            <div class="double-bounce2"></div>
        </div>
    </div>


    <div class="content-wrapper hide-until-loading">
        {% block body %}    {% endblock %}
    </div><!--.content-wrapper end -->
    <footer>
        <div class="footer">

            <div class="container">
                <div class="footer-wrapper">
                    <div class="row">


                        <!-- Footer Col. -->
                        <div class="col-md-4 col-sm-4 footer-col">
                            <div class="footer-content">
                                <div class="footer-content-logo">
                                    {% if site.logo %}
                                        <img alt="Vertex" height="35"
                                             src="{{ vich_uploader_asset(site, 'logoFile') | imagine_filter('logo') }}"/>
                                    {% else %}
                                        <img alt="Vertex" height="35"
                                             src="{{ asset('assets/img/logo_site_default.png') }}"/>
                                    {% endif %}
                                </div>
                                <div class="footer-content-text">
                                    {% if not site.nameInLogo %}
                                        <p><strong>{{ site.name }}</strong></p>
                                    {% endif %}
                                    <p>{{ site.description | raw | nl2br }}</p>
                                </div>
                            </div>
                        </div>
                        <!-- //Footer Col.// -->


                        <!-- Footer Col. -->
                        <div class="col-md-4 col-sm-4 footer-col">
                            <div class="footer-title">
                                Links
                            </div>
                            <div class="footer-content">
                                <ul class="footer-category-list">
                                    <li>
                                        <a href="{{ path('home'|route(app.request.server.get('REQUEST_URI')), siteName) }}">Accueil</a>
                                    </li>
                                    <li>
                                        <a href="{{ path('presentation'|route(app.request.server.get('REQUEST_URI')), siteName) }}">Présentation</a>
                                    </li>
                                    <li>
                                        <a href="{{ path('blog'|route(app.request.server.get('REQUEST_URI')), siteName) }}">Blog</a>
                                    </li>
                                    <li>
                                        <a href="{{ path('devis'|route(app.request.server.get('REQUEST_URI')), siteName) }}">Devis</a>
                                    </li>
                                    <li>
                                        <a href="{{ path('contact'|route(app.request.server.get('REQUEST_URI')), siteName) }}">Contact</a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                        <!-- //Footer Col.// -->

                        <div class="col-md-4 col-sm-4 footer-col">
                            {% if site.hasOneSocialLink %}
                                <div class="footer-content">
                                    <div class="footer-title align-right">
                                        Réseaux sociaux
                                    </div>
                                    <div class="footer-content-text">
                                        <div class="social-icons">
                                            <ul>
                                                {% if site.facebook %}
                                                    <li>
                                                        <a href="{{ site.facebook }}" title="facebook" target="_blank"
                                                           class="social-media-icon facebook-icon">facebook</a>
                                                    </li>
                                                {% endif %}
                                                {% if site.twitter %}
                                                    <li>
                                                        <a href="{{ site.twitter }}" title="twitter" target="_blank"
                                                           class="social-media-icon twitter-icon">twitter</a>
                                                    </li>
                                                {% endif %}
                                                {% if site.instagram %}
                                                    <li>
                                                        <a href="{{ site.instagram }}" title="instagram" target="_blank"
                                                           class="social-media-icon instagram-icon">instagram</a>
                                                    </li>
                                                {% endif %}
                                            </ul>
                                        </div>
                                    </div>
                                </div>
                            {% endif %}
                        </div>
                    </div>
                </div>

            </div>
            <div class="copyright">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12 col-sm-12 center-text">
                            <div class="copyright-text">&copy; {{ "now"|date('Y') }} {{ site.domain }} | Développé par
                                <a
                                        href="http://www.ara-solutions.com" target="_blank">ARA Solutions</a></div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </footer>

</div><!-- wrapper end -->

